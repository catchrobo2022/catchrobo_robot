<?xml version="1.0"?>
<launch>
    <arg name="continue" default="false" />
    <arg name="game_mode" default="normal_game" />
    <arg name="skip" default="false" />
    <param name="skip" value="$(arg skip)" />


    <param name="game_mode" value="$(arg game_mode)" />
    <param name="is_continue" value="$(arg continue)" />

    <node pkg="catchrobo_manager" type="game_manager.py" name="game_manager_node" output="screen" respawn="false" respawn_delay="0" />


</launch>